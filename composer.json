{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "appstract/laravel-options": "^2.2",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "box/spout": "^2.7",
        "fideloper/proxy": "^4.0",
        "joggapp/laravel-mail-viewer": "^5.0",
        "lab404/laravel-impersonate": "^1.2",
        "langleyfoxall/laravel-nist-password-rules": "^4.1",
        "laravel/framework": "6.*",
        "laravel/horizon": "^3.4",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "ohffs/simple-laravel-ldap": "^1.3",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.4",
        "spatie/laravel-activitylog": "^3.1",
        "tightenco/ziggy": "^0.8",
        "uogsoe/ldap-laravel": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "codedungeon/phpunit-result-printer": "^0.26.1",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utils/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
